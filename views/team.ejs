<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Team</title>
  <link rel="stylesheet" href="css/style.css">
</head>

<body>
  <div class="container">
    <h1 class="heading"><span></span>Our Team</h1>

    <div class="profiles">

      <% teams.forEach(function(team){ %>
         <div class="profile">
            <img src=<%= team.img %> class="profile-img">
            <h3 class="user-name"><%= team.username %></h3>
            <h5><%= team.title %></h5>
            <p><%= team.description %></p>
         </div>
     <% }) %>

    </div>
  </div>
</body>

</html>
